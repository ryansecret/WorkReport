<UserControl x:Class="WorkReport.BuildingQuality"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Height="24" MinWidth="24">

    <Grid x:Name="LayoutRoot" Background="White">
        <Rectangle Fill="White" Stroke="Red"  StrokeThickness=".5" StrokeDashArray="4 4"></Rectangle>
        <TextBlock Text="--" x:Name="buildingQC"
                   Foreground="Red"
                   FontSize="15"
                   FontWeight="ExtraBold"
                   VerticalAlignment="Center"
                   FontFamily="Mircosoft YaHei"
                   HorizontalAlignment="Center"
                   ></TextBlock>
    </Grid>
</UserControl>
